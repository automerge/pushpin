{
  "name": "pushpin",
  "productName": "pushpin",
  "version": "1.0.0",
  "description": "A collaborative corkboard app",
  "repository": "https://github.com/inkandswitch/pushpin",
  "main": "src/electron.js",
  "scripts": {
    "start": "electron-forge start",
    "start2": "NAME=userA npm start & NAME=userB npm start",
    "start3": "NAME=userA npm start & NAME=userB npm start & NAME=userC npm start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "test": "electron-mocha --renderer test/**/*.{js,jsx}"
  },
  "keywords": [],
  "author": "inkandswitch",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "pushpin"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "inkandswitch",
        "name": "pushpin"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "pushpin"
      }
    }
  },
  "dependencies": {
    "@ibm/plex": "^1.0.2",
    "automerge": "^0.7.8",
    "bs58": "^4.0.1",
    "classnames": "^2.2.5",
    "codemirror": "^5.37.0",
    "diff-match-patch": "^1.0.1",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "hypercore": "^6.14.0",
    "hypercore-archiver": "^4.6.0",
    "hypercore-protocol": "^6.5.1",
    "hyperdiscovery": "^7.1.0",
    "immutable": "^3.8.2",
    "jimp": "^0.2.28",
    "js-crc": "^0.2.0",
    "line-awesome": "git+https://github.com/choxi/line-awesome.git",
    "mkdirp": "0.5.1",
    "react": "16.4.0",
    "react-color": "^2.14.1",
    "react-contextmenu": "2.9.2",
    "react-dom": "16.4.0",
    "react-hot-loader": "^3.1.3",
    "react-measure": "^2.0.2",
    "react-pdf-js": "^3.0.7",
    "react-rnd": "^7.4.0",
    "react-simple-dropdown": "^3.2.0",
    "react-toggle": "4.0.2",
    "riek": "^1.1.0",
    "tmp": "0.0.33",
    "to-buffer": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "electron-forge": "^5.2.1",
    "electron-mocha": "^6.0.2",
    "electron-prebuilt-compile": "2.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.2.0",
    "react-test-renderer": "^15.6.2"
  }
